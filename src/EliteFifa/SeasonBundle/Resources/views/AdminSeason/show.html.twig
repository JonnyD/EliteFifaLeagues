Associations <a href="{{ path('elite_fifa.admin_create_association', {'seasonId': season.id }) }}">
    create</a> <br />
{% for association in season.associations %}
    <a href="{{ path('elite_fifa.admin_show_association', {'slug': association.slug, 'season': season.number }) }}">
        {{ association.name }}</a>
{% endfor %}
<br />
Competitors <br />
<table>
    <thead></thead>
    <tbody>
    {% for competitor in season.competitors %}
        <tr>
            <td>{{ competitor.id }}</td>
            <td>{% if competitor.hasTeam %}{{ competitor.team.name }}{% endif %}</td>
            <td>{% if competitor.hasUser %}{{ competitor.user.username }}{% endif %}</td>
            <td><a href="{{ path('elite_fifa.remove_user', { 'id': competitor.id }) }}">Remove</a></td>
            <td><a href="{{ path('elite_fifa.change_competitor', { 'id': competitor.id }) }}">Change</a></td>
        </tr>
    {% endfor %}
    </tbody>
</table>